{
	"name": "edgarindustries/youtubefield",
	"description": "Field for selecting YouTube videos. Takes various URL formats as input and stores the YouTube ID.",
	"type": "silverstripe-vendormodule",
	"keywords": ["silverstripe", "youtube", "formfield"],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Ed Linklater",
			"email": "ed@edgar.industries"
		}
	],
	"require": {
		"silverstripe/framework": "^4",
		"guzzlehttp/guzzle": "^6.0 || ^7.0"
	},
	"autoload": {
		"psr-4": {
			"EdgarIndustries\\YouTubeField\\": "src/",
			"EdgarIndustries\\YouTubeField\\Tests\\": "tests/"
		}
	},
	"extra": {
		"expose": [
			"client"
		],
		"installer-name": "youtubefield"
	}
}
